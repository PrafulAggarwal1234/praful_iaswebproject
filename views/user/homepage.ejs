<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Homepage</title>
</head>
<body>
    <h1>Hello TrulyIas</h1>
    <h2>Latest Questions</h2>
    <ul>
        <% questions.forEach(function(question) { %>
            <li>
                <a href="/question/<%= question._id %>">
                    <%= question.title %>
                </a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
